<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.ccg.lab.Entities.Upload" name="Upload">
        <table name="uploads" schema="public" catalog="java"/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="username">
                <column name="username"/>
            </basic>
            <basic name="document">
                <column name="document"/>
            </basic>
            <basic name="documentId">
                <column name="document_id"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
