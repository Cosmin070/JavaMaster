<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
    <persistence-unit name="JPA" transaction-type="RESOURCE_LOCAL">
        <non-jta-data-source>jdbc/java</non-jta-data-source>
        <class>com.lab3.DTOs.ExamsEntity</class>
        <class>com.lab3.DTOs.PresentationEntity</class>
        <class>com.lab3.DTOs.WrittenTestEntity</class>
        <class>com.lab3.DTOs.StudentsEntity</class>
    </persistence-unit>

</persistence>
