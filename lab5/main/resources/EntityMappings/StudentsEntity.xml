<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.lab3.DTOs.StudentsEntity">
        <table name="students" schema="public" catalog="java"/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="firstName">
                <column name="first_name"/>
            </basic>
            <basic name="lastName">
                <column name="last_name"/>
            </basic>
            <basic name="year">
                <column name="year" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
