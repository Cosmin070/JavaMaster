<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.ccg.lab.Entities.User" name="User">
        <table name="users" schema="public" catalog="java"/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="username">
                <column name="username"/>
            </basic>
            <basic name="password">
                <column name="password"/>
            </basic>
            <basic name="role">
                <column name="role"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
