<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="3.0" xmlns="https://jakarta.ee/xml/ns/jakartaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee
                                  https://jakarta.ee/xml/ns/jakartaee/web-facesconfig_3_0.xsd">
    <navigation-rule>
        <from-view-id>/faces/unregistered/main.xhtml</from-view-id>
        <navigation-case>
            <to-view-id>/faces/unregistered/login.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <to-view-id>/faces/unregistered/createUser.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/faces/unregistered/login.xhtml</from-view-id>
        <navigation-case>
            <to-view-id>/faces/admin/main.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <to-view-id>/faces/user/main.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/faces/admin/main.xhtml</from-view-id>
        <navigation-case>
            <to-view-id>/faces/admin/createUpload.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <to-view-id>/faces/admin/createUser.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <to-view-id>/faces/admin/viewUpload.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/faces/user/main.xhtml</from-view-id>
        <navigation-case>
            <to-view-id>/faces/admin/createUpload.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>